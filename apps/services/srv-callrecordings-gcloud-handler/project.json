{
  "name": "svc-srv-callrecordings-gcloud-handler",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/services/srv-callrecordings-gcloud-handler/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "platform": "node",
        "generatePackageJson": true,
        "outputPath": "dist/apps/services/srv-callrecordings-gcloud-handler",
        "format": ["cjs"],
        "bundle": false,
        "main": "apps/services/srv-callrecordings-gcloud-handler/src/main.ts",
        "tsConfig": "apps/services/srv-callrecordings-gcloud-handler/tsconfig.app.json",
        "assets": [
          "apps/services/srv-callrecordings-gcloud-handler/src/assets"
        ],
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "generateLockfile": true,
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            }
          }
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "services-srv-callrecordings-gcloud-handler:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "services-srv-callrecordings-gcloud-handler:build:development"
        },
        "production": {
          "buildTarget": "services-srv-callrecordings-gcloud-handler:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/services/srv-callrecordings-gcloud-handler/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/services/srv-callrecordings-gcloud-handler/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "docker-build": {
      "dependsOn": ["build"],
      "command": "docker build -f apps/services/srv-callrecordings-gcloud-handler/Dockerfile . -t svc-srv-callrecordings-gcloud-handler"
    }
  },
  "tags": []
}
