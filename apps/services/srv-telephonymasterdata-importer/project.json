{
  "name": "svc-srv-telephonymasterdata-importer",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/services/srv-telephonymasterdata-importer/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",

      "options": {
        "platform": "node",
        "generatePackageJson": true,
        "outputPath": "dist/apps/services/srv-telephonymasterdata-importer",
        "format": ["cjs"],
        "bundle": false,
        "main": "apps/services/srv-telephonymasterdata-importer/src/main.ts",
        "tsConfig": "apps/services/srv-telephonymasterdata-importer/tsconfig.app.json",
        "assets": [
          {
            "input": "apps/services/srv-telephonymasterdata-importer/src/env",
            "glob": ".env.*",
            "output": "apps/services/srv-telephonymasterdata-importer/src/env"
          }
        ],

        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "development": {},
        "production": {
          "generateLockfile": true,
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            }
          }
        }
      }
    },
    "serve": {
      "executor": "@nrwl/js:node",
      "defaultConfiguration": "development",
      "port": 8081,
      "options": {
        "buildTarget": "services-srv-telephonymasterdata-importer:build"
      },
      "envFile": ".env.development",
      "configurations": {
        "development": {
          "buildTarget": "services-srv-telephonymasterdata-importer:build:development"
        },
        "production": {
          "buildTarget": "services-srv-telephonymasterdata-importer:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "apps/services/srv-telephonymasterdata-importer/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/services/srv-telephonymasterdata-importer/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "docker-build": {
      "dependsOn": ["build"],
      "command": "docker build -f apps/services/srv-telephonymasterdata-importer/Dockerfile . -t svc-srv-telephonymasterdata-importer"
    }
  },
  "tags": []
}
